{
  "name": "@fransvilhelm/mjml-react",
  "version": "0.0.0",
  "description": "",
  "author": "Adam Bergman <adam@fransvilhelm.com>",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "imports": "./dist/index.mjs",
    "require": "./dist/index.js"
  },
  "scripts": {
    "prebuild": "npm run generate",
    "build": "tsup --dts",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "generate": "node -r esbuild-register ./scripts/generate.ts",
    "lint": "eslint . --ext .ts --ext .tsx",
    "format": "prettier --write '**/*.{ts,tsx}'",
    "release": "npm run build && npx changeset publish",
    "prepare": "husky install"
  },
  "peerDependencies": {
    "mjml": "^4.13.0",
    "mjml-core": "^4.13.0",
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.24.2",
    "@fransvilhelm/changeset-changelog": "^1.1.1",
    "@trivago/prettier-plugin-sort-imports": "^3.3.0",
    "@types/lodash": "^4.14.182",
    "@types/mjml": "^4.7.0",
    "@types/mjml-core": "^4.7.1",
    "@types/node": "^18.6.5",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "cheerio": "^1.0.0-rc.12",
    "esbuild": "^0.14.54",
    "esbuild-register": "^3.3.3",
    "eslint": "^8.21.0",
    "eslint-config-react-app": "^7.0.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "lodash": "^4.17.21",
    "mjml": "^4.13.0",
    "mjml-core": "^4.13.0",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^6.2.1",
    "typescript": "^4.7.4",
    "vitest": "^0.21.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
