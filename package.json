{
  "name": "@fransvilhelm/mjml-react",
  "version": "0.0.0",
  "description": "",
  "author": "Adam Bergman <adam@fransvilhelm.com>",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "imports": "./dist/index.mjs",
    "require": "./dist/index.js"
  },
  "scripts": {
    "prebuild": "npm run generate",
    "build": "tsup --dts",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "generate": "node -r esbuild-register ./scripts/generate.ts",
    "lint": "eslint . --ext .ts --ext .tsx",
    "format": "prettier --write '**/*.{ts,tsx}'",
    "release": "npm run build && npx changeset publish",
    "prepare": "husky install",
    "example": "node -r esbuild-register ./example/server.ts",
    "example:watch": "NODE_ENV=development npx nodemon --watch example --exec 'npm run example' -e ts,tsx"
  },
  "peerDependencies": {
    "mjml": "^4.13.0",
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@fransvilhelm/changeset-changelog": "^1.1.1",
    "@testing-library/dom": "^9.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/marked": "^4.0.8",
    "@types/mjml": "^4.7.0",
    "@types/mjml-core": "^4.7.1",
    "@types/node": "^18.15.10",
    "@types/prettier": "^2.7.2",
    "@types/react": "^18.0.29",
    "@types/react-dom": "^18.0.11",
    "c8": "^7.13.0",
    "cheerio": "^1.0.0-rc.12",
    "esbuild": "^0.17.14",
    "esbuild-register": "^3.4.2",
    "eslint": "^8.36.0",
    "eslint-config-react-app": "^7.0.1",
    "husky": "^8.0.3",
    "jsdom": "^21.1.1",
    "lint-staged": "^13.2.0",
    "marked": "^4.3.0",
    "micro": "^10.0.1",
    "mjml": "^4.13.0",
    "mjml-core": "^4.13.0",
    "prettier": "^2.8.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.2",
    "vitest": "^0.29.7"
  },
  "publishConfig": {
    "access": "public"
  }
}
